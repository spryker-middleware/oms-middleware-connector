{
    "name": "spryker-middleware/oms-middleware-connector",
    "description": "OmsMiddlewareConnector module",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "spryker-middleware/process": "dev-dev",
        "spryker/oms": "^8.0.0",
        "spryker/sales": "^8.0.0"
    },
    "require-dev": {
        "spryker/calculation": "*",
        "spryker/cart": "*",
        "spryker/code-sniffer": "*",
        "spryker/propel": "*",
        "spryker/silex": "*",
        "spryker/testify": "*",
        "spryker/transfer": "*",
        "spryker/architecture-sniffer": "dev-master",
        "phpmd/phpmd": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "SprykerMiddleware\\": "src/SprykerMiddleware/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/spryker-middleware/process.git"
        },
        {
            "type": "git",
            "url": "https://github.com/spryker-middleware/logger.git"
        },
        {
            "type": "git",
            "url": "https://github.com/spryker-middleware/report.git"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 600
    }
}
